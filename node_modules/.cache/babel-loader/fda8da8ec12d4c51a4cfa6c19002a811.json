r news_CN_list = [];\n\n      if (news_CN) {\n        // 当前页\n        var curpage = this.page_news_CN.current_page; // 每页显示数目\n\n        var size = this.page_news_CN.page_size; // 开始和结尾的索引\n\n        var begin_index = size * (curpage - 1);\n        var end_index = size * curpage; // 假分页，切分数据\n\n        if (end_index >= news_CN.length) {\n          // 若最后一页未铺满\n          news_CN_list = news_CN.slice(begin_index);\n        } else {\n          news_CN_list = news_CN.slice(begin_index, end_index);\n        }\n      }\n\n      return news_CN_list;\n    },\n    news_CN_data_length: function news_CN_data_length() {\n      var news_CN = this.result.CN_News;\n      if (news_CN) return news_CN.length;else return 0;\n    }\n  },\n  methods: {\n    page_news_CN_change: function page_news_CN_change(currentPage) {\n      this.page_news_CN.current_page = currentPage;\n    },\n    page_news_CN_change_size: function page_news_CN_change_size(size) {\n      this.page_news_CN.page_size = size;\n    }\n  }\n};",{"version":3,"sources":["ItemNewsCN.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoCA,OAAA,gBAAA,MAAA,mCAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,0BAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,YAAA,EAAA;AACA,QAAA,YAAA,EAAA,CADA;AAEA,QAAA,UAAA,EAAA,CAAA,EAAA,EAAA,EAAA,CAFA;AAGA,QAAA,SAAA,EAAA,EAHA;AAIA;AACA,QAAA,WAAA,EAAA;AALA;AADA,KAAA;AASA,GAdA;AAeA,EAAA,QAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,aAAA,KAAA,IAAA,CAAA,UAAA,EAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,UAAA,OAAA,GAAA,KAAA,MAAA,CAAA,OAAA;AACA,UAAA,YAAA,GAAA,EAAA;;AACA,UAAA,OAAA,EAAA;AACA;AACA,YAAA,OAAA,GAAA,KAAA,YAAA,CAAA,YAAA,CAFA,CAGA;;AACA,YAAA,IAAA,GAAA,KAAA,YAAA,CAAA,SAAA,CAJA,CAKA;;AACA,YAAA,WAAA,GAAA,IAAA,IAAA,OAAA,GAAA,CAAA,CAAA;AACA,YAAA,SAAA,GAAA,IAAA,GAAA,OAAA,CAPA,CAQA;;AACA,YAAA,SAAA,IAAA,OAAA,CAAA,MAAA,EAAA;AACA;AACA,UAAA,YAAA,GAAA,OAAA,CAAA,KAAA,CAAA,WAAA,CAAA;AACA,SAHA,MAGA;AACA,UAAA,YAAA,GAAA,OAAA,CAAA,KAAA,CAAA,WAAA,EAAA,SAAA,CAAA;AACA;AACA;;AACA,aAAA,YAAA;AACA,KAxBA;AAyBA,IAAA,mBAzB