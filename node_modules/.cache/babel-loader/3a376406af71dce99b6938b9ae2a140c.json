r size = this.page_paper.page_size; // 开始和结尾的索引\n\n        var begin_index = size * (curpage - 1);\n        var end_index = size * curpage; // 假分页，切分数据\n\n        if (end_index >= papers.length) {\n          // 若最后一页未铺满\n          papers_list = papers.slice(begin_index);\n        } else {\n          papers_list = papers.slice(begin_index, end_index);\n        }\n      }\n\n      return papers_list;\n    },\n    paper_data_length: function paper_data_length() {\n      var papers = this.result.papers;\n      if (papers) return papers.length;else return 0;\n    }\n  },\n  methods: {\n    page_paper_change: function page_paper_change(currentPage) {\n      this.page_paper.current_page = currentPage;\n    },\n    page_paper_change_size: function page_paper_change_size(size) {\n      this.page_paper.page_size = size;\n    }\n  }\n};",{"version":3,"sources":["ItemPaper.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuDA,OAAA,gBAAA,MAAA,mCAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,0BAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,UAAA,EAAA;AACA,QAAA,YAAA,EAAA,CADA;AAEA,QAAA,UAAA,EAAA,CAAA,EAAA,EAAA,EAAA,CAFA;AAGA,QAAA,SAAA,EAAA,EAHA;AAIA;AACA,QAAA,WAAA,EAAA;AALA;AADA,KAAA;AASA,GAdA;AAeA,EAAA,QAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,aAAA,KAAA,IAAA,CAAA,UAAA,EAAA;AACA,KAHA;AAIA,IAAA,UAJA,wBAIA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA;AACA,UAAA,WAAA,GAAA,EAAA;;AACA,UAAA,MAAA,EAAA;AAAA;AACA;AACA,YAAA,OAAA,GAAA,KAAA,UAAA,CAAA,YAAA,CAFA,CAGA;;AACA,YAAA,IAAA,GAAA,KAAA,UAAA,CAAA,SAAA,CAJA,CAKA;;AACA,YAAA,WAAA,GAAA,IAAA,IAAA,OAAA,GAAA,CAAA,CAAA;AACA,YAAA,SAAA,GAAA,IAAA,GAAA,OAAA,CAPA,CAQA;;AACA,YAAA,SAAA,IAAA,MAAA,CAAA,MAAA,EAAA;AACA;AACA,UAAA,WAAA,GAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA;AACA,SAHA,MAGA;AACA,UAAA,WAAA,GAAA,MAAA,CAAA,KAAA,CAAA,WAAA,EAAA,SAAA,CAAA;AACA;AACA;;AACA,aAAA,WAAA;AACA,KAxBA;AAyBA,IAAA,iBAzBA,+BAyBA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA;AACA,UAAA,MAAA,EACA,OAAA,MAAA,CAAA,MAAA,CADA,KAGA,OAAA,CAAA;AACA;AA/BA,GAfA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,iBADA,6BACA,WADA,EACA;AACA,WAAA,UAAA,CAAA,YAAA,GAAA,WAAA;AACA,KAHA;AAIA,IAAA,sBAJA,kCAIA,IAJA,EAIA;AACA,WAAA,UAAA,CAAA,SAAA,GAAA,IAAA;AACA;AANA;AAhDA,CAAA","sourcesContent":["<template>\n  <!-- paper条目 -->\n  <div>\n    <div class=\"data-item\" v-for=\"(paper, index) in paper_data\" :key=index>\n      <!--标题-->\n      <div v-if=\"paper.paperTitle!=''\"><a :href=\"paper.paperUrl\" target=\"_blank\" class=\"result-title\" v-html=\"paper.paperTitle\"></a>\n        <!-- <span class='icon-type bgColor-paper'>paper</span> -->\n      </div>\n      <!-- 作者 -->\n      <p v-if=\"paper.paperAuthors!=''\"><span class=\"word-field\">Authors：</span>\n        <!-- 循环取出数组中的作者名 -->\n        <span v-for=\"(author,index) in paper.paperAuthors\" :key=index>\n          <span class=\"authors-inventor-source author-span-margin\">{{author}},</span>\n        </span>\n      </p>\n      <!-- 摘要 -->\n      <!-- <p v-if=\"paper.paperAbstract!=''\"><span class=\"word-field word-field-abstract\">Abstract：</span><span class=\"result-content\">{{paper.paperAbstract}}</span></p> -->\n      <p v-if=\"paper.paperAbstract!=''\">\n        <span class=\"word-field word-field-abstract\">Abstract：</span>\n        <text-expand-shrink :text=\"paper.paperAbstract\" :key=\"paper.paperTitle\"></text-expand-shrink>\n      </p>\n      <!--出版年份-->\n      <p v-if=\"paper.paperPublishYear>=0\">\n        <span class=\"word-field\">Publish year: </span>\n        <span class='result-content'>{{paper.paperPublishYear}}</span>\n      </p>\n      <!-- 被引次数 -->\n      <p v-if=\"paper.paperCitationsNum>=0\">\n        <span class=\"word-field\">Citation: </span>\n        <span class='result-content'>{{paper.paperCitationsNum}}</span>\n      </p>\n      <!-- 期刊   -->\n      <p v-if=\"paper.paperJournalFullName!=''\">\n        <span class='word-field'>Journal: </span>\n        <span class='paper-reslt word-field-abstract' v-html=\"paper.paperJournalFullName\"></span>\n      </p>\n      <!-- 会议 -->\n      <p v-if=\"paper.paperConfFullName!=''\">\n        <span class='word-field'>Conference: </span>\n        <span class='result-content word-field-abstract' v-html=\"paper.paperConfFullName\"></span>\n      </p>\n    </div>\n    <!-- 分页 -->\n    <el-pagination \n      :current-page=\"page_paper.current_page\" \n      :page-sizes=\"page_paper.page_sizes\" \n      @current-change=\"page_paper_change\"\n      @size-change=\"page_paper_change_size\" \n      :layout=\"page_paper.page_layout\" \n      :total=\"paper_data_length\">\n    </el-pagination>\n  </div>\n</template>\n\n<script>\n  import textExpandShrink from \"@/components/textExpandShrink.vue\"\n\n  export default {\n    components: {\n      \"text-expand-shrink\": textExpandShrink\n    },\n    data() {\n      return {\n        page_paper: {\n          current_page: 1,\n          page_sizes: [10, 15],\n          page_size: 10,\n          // page_layout: \"sizes, prev, pager, next, total\",\n          page_layout: \"prev, pager, next, total\"\n        }\n      }\n    },\n    computed: {\n      result() {\n        return this.$bus.get_result()\n      },\n      paper_data() {\n        let papers = this.result.papers;\n        let papers_list = [];\n        if (papers) { // 若有 paper 数据\n          // 当前页\n          let curpage = this.page_paper.current_page;\n          // 每页显示数目\n          let size = this.page_paper.page_size;\n          // 开始和结尾的索引\n          let begin_index = size * (curpage - 1);\n          let end_index = size * curpage;\n          // 假分页，切分数据\n          if (end_index >= papers.length) {\n            // 若最后一页未铺满\n            papers_list = papers.slice(begin_index);\n          } else {\n            papers_list = papers.slice(begin_index, end_index);\n          }\n        }\n        return papers_list;\n      },\n      paper_data_length() {\n        let papers = this.result.papers;\n        if (papers)\n          return papers.length;\n        else\n          return 0\n      }\n    },\n    methods: {\n      page_paper_change(currentPage) {\n        this.page_paper.current_page = currentPage;\n      },\n      page_paper_change_size(size) {\n        this.page_paper.page_size = size;\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/components/display_info/display_item"}]}