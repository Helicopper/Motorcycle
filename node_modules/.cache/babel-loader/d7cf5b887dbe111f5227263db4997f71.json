         </el-card>\n          </el-col>\n          <el-col :sm=\"24\" :md=\"14\">\n            <!-- 名片 副信息 -->\n            <div class=\"card-sub-info\">\n              <!-- <div \n                v-if=\"info_card_sub.data\"\n                class=\"sub-data\">\n                <p \n                  v-for=\"(detail, index) in info_card_sub.data\"\n                  :key=\"index\"\n                >{{detail}}</p>\n              </div>\n              <div v-if=\"info_card_sub.graphName\"\n                class=\"sub-graph\"></div> -->\n              <dimen-item :data=\"info_card_sub\"></dimen-item>\n            </div>\n          </el-col>\n        </el-row>\n      </div>\n      <!-- 数据信息 -->\n      <div class=\"info-data\">\n        <div\n          class=\"dimen-item\"\n          v-for=\"(data, index) in dimensions_data\"\n          :key=\"index\"\n        >\n          <el-card shadow=\"hover\">\n            <dimen-item\n              :data=\"data\"></dimen-item>\n          </el-card>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  // import DimensionItem from \"./comps/DimensionItem.vue\"\n  export default {\n    name: \"DisplayMedic\",\n    components: {\n      // \"dimen-item\": DimensionItem\n      \"dimen-item\": () => import(\"./comps/DimensionItem.vue\")\n    },\n    created() {\n      // let query = this.$route.query.query;\n      // let url = \"/json/medic_data_\" + query + \".json\";\n      // this.$axios.get(url)\n      //   .then(res => {\n      //     this.$bus.clean_last_result();\n      //     this.$bus.query_data.query_result = res.data;\n      //   })\n      //   .catch(err => {\n      //     console.log(\"err: \" + err);\n      //   })\n      this.search();\n    },\n    watch: {\n      $route() {\n        if(this.$route.query.query) {\n          this.search();\n        }\n      }\n    },\n    computed: {\n      info_card() {\n        if(this.$bus.get_result().info_card) {\n          return this.$bus.get_result().info_card;\n        }\n        return \"\";\n      },\n      info_card_sub() {\n        if(this.$bus.get_result().info_card_sub) {\n          return this.$bus.get_result().info_card_sub;\n          console.log(\"have sub\");\n        }\n        return \"\";\n      },\n      dimensions_data() {\n        if(this.$bus.get_result().dimensions_data) {\n          return this.$bus.get_result().dimensions_data;\n        }\n        return \"\";\n      }\n    },\n    methods: {\n      search() {\n        let query = this.$route.query.query;\n        let url = \"/json/medic_data_\" + query + \".json\";\n        this.$axios.get(url)\n          .then(res => {\n            this.$bus.clean_last_result();\n            this.$bus.query_data.query_result = res.data;\n          })\n          .catch(err => {\n            console.log(\"err: \" + err);\n          })\n      }\n    }\n  }\n</script>\n\n<style lang=\"less\" scoped>\n#display-medic {\n  .info-card {\n    margin: 20px 0;\n    .card-main-info {\n      margin-right: 10px;\n\n      img {\n        // width: 200px;\n        width: 100%;\n        max-width: 200px;\n      }\n      .info-card-detail {\n        margin: 1rem;\n        font-size: 1rem;\n\n        .detail-item {\n          margin-bottom: .5rem; \n\n          .title {\n            // font-weight: bold;\n            margin-right: 5px;\n            text-transform: uppercase;\n          }\n          .content {\n            font-size: 1.2rem;\n          }\n        }\n      }\n    }\n\n    .card-sub-info {\n      float: right;\n      width: 100%;\n      max-width: 400px;\n      // max-height: 300px;\n\n      &::after {\n        content: \" \";\n        display: block;\n      }\n    }\n  }\n\n  .info-data {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n\n    .dimen-item {\n      width: 30%;\n      min-width: 350px;\n      margin-right: 3%;\n      margin-bottom: 20px;\n    }\n  }\n}\n</style>"],"sourceRoot":"src/views/transplant"}]}